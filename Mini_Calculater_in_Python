# Mini Calculater

first = input("Enter first number:")
operator = input("+,-,*,/,%")
second = input("Enter second number:")

first = int(first)
second = int(second)

if operator == "+":
    print(first + second)
elif operator == "-":
    print(first - second)
elif operator == "/":
    print(first / second)
elif operator == "*":
    print(first * second)

else:
    print(Invaild_Error)